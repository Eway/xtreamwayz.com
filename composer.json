{
    "name": "xtreamwayz/xtreamwayz.com",
    "type": "project",
    "homepage": "https://github.com/xtreamwayz/xtreamwayz.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Geert Eltink",
            "homepage": "https://xtreamwayz.github.io/"
        }
    ],
    "require": {
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "~1.0.0@rc || ^1.0",
        "zendframework/zend-expressive-helpers": "^1.2",
        "zendframework/zend-expressive-fastroute": "^1.0",
        "zendframework/zend-expressive-twigrenderer": "^1.0",
        "zendframework/zend-stdlib": "^2.7",
        "zendframework/zend-servicemanager": "dev-develop",
        "ocramius/proxy-manager": "^1.0",
        "tedivm/stash": "^0.13",
        "guzzlehttp/guzzle": "^6.1",
        "mnapoli/front-yaml": "^1.5",
        "phing/phing": "^2.12",
        "monolog/monolog": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "^2.3",
        "filp/whoops": "^1.1",
        "etsy/phan": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "rm -f data/cache/app_config.*",
            "rm -rf data/cache/stash/",
            "rm -rf data/cache/twig/"
        ],
        "post-update-cmd": [
            "rm -f data/cache/app_config.*",
            "rm -rf data/cache/stash/",
            "rm -rf data/cache/twig/"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Domain\\": "src/Domain/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/",
            "DomainTest\\": "test/DomainTest/"
        }
    }
}
